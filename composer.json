{
  "require": {
    "php": "^8.2",
    "faslatam/alpine-js": "^3.14",
    "flightphp/core": "^3.13",
    "illuminate/container": "^11.43",
    "leafs/session": "^2.3",
    "megacreativo/cedulave-api": "^1.1",
    "phar-io/manifest": "^2.0",
    "twbs/bootstrap": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "Views\\": "views"
    },
    "classmap": [
      "app/App.php"
    ],
    "files": [
      "app/utils.php"
    ]
  },
  "scripts": {
    "dev:win": "start php -S localhost:80",
    "migrate:fresh": "php app/commands/migrate-fresh.php",
    "post-install-cmd": [
      "php -r \"if (!file_exists('.env.php')) copy('.env.dist.php', '.env.php');\""
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "rector/rector": "^2.0",
    "symfony/var-dumper": "^7.2"
  },
  "config": {
    "sort-packages": true
  }
}
