{
  "require": {
    "php": ">=8.2",
    "flightphp/core": "^3.8",
    "phar-io/manifest": "^2.0",
    "leafs/session": "^2.0",
    "filp/whoops": "^2.15",
    "megacreativo/cedulave-api": "^1.1",
    "twbs/bootstrap": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "Views\\": "views"
    },
    "classmap": [
      "app/App.php"
    ],
    "files": [
      "app/utils.php"
    ]
  },
  "scripts": {
    "dev:win": "start php -S localhost:80",
    "db:install": "php app/commands/db-install.php",
    "post-install-cmd": [
      "php -r \"if (!file_exists('.env.php')) copy('.env.dist.php', '.env.php');\""
    ]
  },
  "require-dev": {
    "symfony/var-dumper": "^7.0"
  }
}
